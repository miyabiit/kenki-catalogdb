<%= simple_form_for file_prop do |f| %>
  <%= f.button :submit, class: 'float-right btn btn-primary' %>
  <%= link_to '<戻る', file_props_path, class: 'btn btn-outline-primary', style: 'margin-bottom: 10px' %>
    <h3><%= title %></h3>
  <div class="clearfix">
    <%= render('shared/validation_alert', model: file_prop) if file_prop.errors.any? %>
    <%= f.input :id, wrapper: :horizontal_plaintext unless f.object.new_record? %>
    <%= f.input :name %>
    <%= f.input :file %>
  </div>
<% end %>
