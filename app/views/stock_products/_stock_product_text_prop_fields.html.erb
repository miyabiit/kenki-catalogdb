<div class="nested-fields form-group row">
  <div class="col-md-6">
    <%= f.collection_select :stored_prop_id, TextProp.accessible_by(current_ability), :id, :name, {prompt: ''}, class: 'form-control' %>
  </div>
  <div class="col-md-2">
    <%= f.input :published %>
  </div>
  <div class="col-md-2">
    <%= f.input :charterable %>
  </div>
  <div class="col-md-2">
    <%= link_to_remove_association "削除", f, class: 'btn btn-danger' %>
  </div>
</div>
