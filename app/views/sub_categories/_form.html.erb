<%= simple_form_for sub_category do |f| %>
  <%= f.button :submit, class: 'float-right btn btn-primary' %>
  <%= link_to '<戻る', sub_categories_path, class: 'btn btn-outline-primary', style: 'margin-bottom: 10px' %>
    <h3><%= title %></h3>
  <div class="clearfix">
    <%= render('shared/validation_alert', model: sub_category) if sub_category.errors.any? %>
    <%= f.input :id, wrapper: :horizontal_plaintext unless f.object.new_record? %>
    
      <%= f.input :name %>
    
  </div>
<% end %>
